@model List<SysPageModel>
@{
    if (Model.IsEmpty()) return;
}
<section class="dok-breadcrumb">
    <div class="container">
        <div class="breadcrumb" id="breadcrumb">
            <a href="/"><i class="fa fa-home-alt me-1 main-color"></i></a>
            @for (int i = 0; Model.IsNotEmpty() && i < Model.Count; i++)
            {
                <span class="mx-2"><i class="fa fa-chevron-right fz-12"></i></span>
                <a href="@Html.GetUrl(Model[i].Code)">@(Model[i].Name)</a>
            }
        </div>
    </div>
</section>